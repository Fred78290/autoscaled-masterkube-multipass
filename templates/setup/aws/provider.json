	"token": "${AWS_TOKEN}",
	"profile": "${AWS_PROFILE}",
	"region": "${AWS_REGION}",
	"keyName": "${SSH_KEYNAME}",
	"ami": "${TARGET_IMAGE_AMI}",
	"iam-role-arn": "${WORKER_INSTANCE_PROFILE_ARN}",
	"timeout": 120,
